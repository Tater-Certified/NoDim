    "client": [],
    "server": []
  },
  "mixins": [
    "nodim.mixins.vanilla.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}"
  }
